{
  "name": "vite-plugin-koa-mocks",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.15.0",
  "description": "Serve mock API with Koa.js in Vite projects.",
  "author": "Abu1999",
  "license": "MIT",
  "homepage": "https://github.com/Abu1999/vite-plugin-koa-mock#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Abu1999/vite-plugin-koa-mock.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "koa.js",
    "mock"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean && node scripts/inject_type.js",
    "test": "vitest",
    "test:cover": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.2",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^4.0.3",
    "@types/koa__router": "^12.0.4",
    "chokidar": "^4.0.3",
    "get-port": "^7.1.0",
    "importx": "^0.5.2",
    "koa": "^2.16.2",
    "koa-bodyparser": "^4.4.1",
    "koa-compose": "^4.1.0",
    "kolorist": "^1.8.0",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.37.0",
    "@types/koa-compose": "^3.2.8",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^8.57.1",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.5.14",
    "vitest": "^0.29.8"
  }
}
