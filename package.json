{
  "name": "vite-plugin-koa-mock",
  "type": "module",
  "version": "0.9.3",
  "packageManager": "pnpm@7.30.0",
  "description": "Serve mock API with Koa.js in Vite projects.",
  "author": "mys1024",
  "license": "MIT",
  "homepage": "https://github.com/mys1024/vite-plugin-koa-mock#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mys1024/vite-plugin-koa-mock.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "koa.js",
    "mock"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "test": "vitest",
    "test:cover": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.37.0",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.7",
    "eslint": "^8.36.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "koa": "^2.14.1",
    "kolorist": "^1.7.0"
  }
}
